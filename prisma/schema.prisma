generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql" // أو "mysql" حسبك
  url      = env("DATABASE_URL")
}

model Provider {
  id         String   @id @default(cuid())
  name       String
  lang       String
  img        String?
  keywords   Json
  isActive   Boolean  @default(true)
}
